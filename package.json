{
  "name": "demo-tauri",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:web": "vite",
    "dev:desktop": "tauri dev",
    "build:web": "tsc && vite build",
    "check": "pnpm check:fmt && pnpm check:lint",
    "check:fmt": "cd src-tauri && cargo make check-format",
    "check:lint": "cd src-tauri && cargo make clippy",
    "build:macos:arm64": "tauri build --target aarch64-apple-darwin",
    "build:macos:x86_64": "tauri build --target x86_64-apple-darwin",
    "build:macos:universal": "tauri build --target universal-apple-darwin",
    "build:macos": "pnpm build:macos:arm64 && pnpm build:macos:x86_64 && pnpm build:macos:universal"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "@tauri-apps/cli": "^2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0"
}
